{
	"name": "Stage1_employeefile_structure_check",
	"properties": {
		"activities": [
			{
				"name": "GoodEmployeeTemplate",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "employeeshemaonly",
						"type": "DatasetReference"
					},
					"fieldList": [
						"structure",
						"columnCount",
						"exists",
						"lastModified"
					],
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "DelimitedTextReadSettings"
					}
				}
			},
			{
				"name": "AllNewEmployeefiles",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"fieldList": [
						"childItems"
					],
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "DelimitedTextReadSettings"
					}
				}
			},
			{
				"name": "NewEmployeeFiles",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "AllNewEmployeefiles",
						"dependencyConditions": [
							"Succeeded"
						]
					},
					{
						"activity": "GoodEmployeeTemplate",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('AllNewmoviefiles').output.childItems\n",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "CurrentFile",
							"type": "GetMetadata",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "currentLoopFilename",
									"value": "@item().name"
								}
							],
							"typeProperties": {
								"dataset": {
									"referenceName": "Newemployeefilesfolder",
									"type": "DatasetReference",
									"parameters": {
										"filename": {
											"value": "@item().name",
											"type": "Expression"
										}
									}
								},
								"fieldList": [
									"structure",
									"itemName"
								],
								"storeSettings": {
									"type": "AzureBlobFSReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings"
								}
							}
						},
						{
							"name": "If Condition1",
							"type": "IfCondition",
							"dependsOn": [
								{
									"activity": "CurrentFile",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"expression": {
									"value": "@equals(activity('CurrentFile').output.structure,activity('GoodMovieTemplate').output.structure)",
									"type": "Expression"
								},
								"ifFalseActivities": [
									{
										"name": "Fail1",
										"type": "Fail",
										"dependsOn": [],
										"userProperties": [],
										"typeProperties": {
											"message": {
												"value": "@activity('CurrentFile').output.itemName",
												"type": "Expression"
											},
											"errorCode": "111"
										}
									}
								],
								"ifTrueActivities": [
									{
										"name": "Execute stage1 pipline",
										"type": "ExecutePipeline",
										"dependsOn": [],
										"userProperties": [
											{
												"name": "currentfilename",
												"value": "@activity('CurrentFile').output.itemName"
											}
										],
										"typeProperties": {
											"pipeline": {
												"referenceName": "Stage1_goodemployeefile_copy",
												"type": "PipelineReference"
											},
											"waitOnCompletion": true,
											"parameters": {
												"filename": {
													"value": "@activity('CurrentFile').output.itemName",
													"type": "Expression"
												}
											}
										}
									}
								]
							}
						}
					]
				}
			}
		],
		"annotations": []
	}
}